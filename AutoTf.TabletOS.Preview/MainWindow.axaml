<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:viewModels="clr-namespace:AutoTf.TabletOS.Preview;assembly=AutoTf.TabletOS.Avalonia"
        xmlns:preview="clr-namespace:AutoTf.TabletOS.Preview"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="AutoTf.TabletOS.Preview.MainWindow"
        Height="800"
        Width="1280"
        Title="AutoTf.TabletOS.Preview"
        x:DataType="preview:MainWindowViewModel">
